<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Booking Trips</title>
<link rel="stylesheet" href="../reset.css">

    <link rel="stylesheet" href="../bookinfos.css">
    <!-- <link rel="stylesheet" href="./bookinfos.css"> -->
    <link rel="stylesheet" href="./header_abdallah.css">
    <link rel="stylesheet" href="./esraa/login.css">
    <link rel="stylesheet" href="./esraa/sign.css">

</head>

<body>
    <header>

        <%- include('../layout/header_abdallah.ejs') %> 
        <%- include('../layout/signup.ejs')%>
        <%- include('../layout/login.ejs')%>
        
    </header>
    <main>
        <% data.forEach (val  =>{ %>
        <div>
            <h1> <%= val.name %></h1>
            <img src="<%= val.image %>" alt="">
            <p class="pad"> <%= val.net  %></p>
            <p class="pad"> <%= val.agencies%> </p>
            <p class="pad"> <%=val.description%> </p>
            <p class="pad sp">cost: <%= val.cost%> $</p>
            <form action="/delete/<%= val.id%>?_method=delete" method="post">
            <button type="submit">delete</button>
            </form>
        </div>
        <%});%>



    </main>

</body>

</html>